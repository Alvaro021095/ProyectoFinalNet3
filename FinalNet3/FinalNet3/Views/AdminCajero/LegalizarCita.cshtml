@{
    ViewBag.Title = "Cajero";
}

<script defer src="@Url.Content("~/Scripts/System/Cajero/Cajero.js")" type="text/javascript"></script>


<h2>Legalizacion de Citas</h2>
<input id="txtId" type="hidden" />
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="row clearfix">
        <div class=" col-lg-4 col-md-4 col-sm-4 col-xs-4">
            <div class="">
                <div class="">
                    <label>
                        Documento del Paciente
                    </label>
                    <input class="form-control"
                           id="txtDocumento" required />
                </div>
            </div>
        </div>
        <div class=" col-lg-4 col-md-4 col-sm-4 col-xs-4">
            <div class="">
                <div class="">
                    <label>
                        Numero de Cita
                    </label>
                    <input class="form-control"
                           id="txtNumero" required />
                </div>
            </div>
        </div>
       
    </div>

</div>
<br />
<div class="container">

    <button type="button" class="btn btn-success" onclick="Legalizar();">Legalizar Cita</button>

</div>

<br />
