<h2>Ingresos Economicos</h2>
<input id="txtId" type="hidden" />
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="row clearfix">
        <div class=" col-lg-6 col-md-6 col-sm-6 col-xs-6">
            <div class="">
                <div class="">
                    <label>
                        Nombre
                    </label>
                    <input class="form-control"
                           id="txtNombre" required />
                </div>
            </div>
        </div>
        <div class=" col-lg-6 col-md-6 col-sm-6 col-xs-6">
            <div class="">
                <div class="">
                    <label>
                        Categoria
                    </label>
                    <input class="form-control"
                           id="txtCategoria" required />
                </div>
            </div>
        </div>
        <div class=" col-lg-6 col-md-6 col-sm-6 col-xs-6">
            <div class="">
                <div class="">
                    <label>
                        Valor Categoria Cita
                    </label>
                    <input class="form-control"
                           id="txtValorCatCita" required />
                </div>
            </div>
        </div>
        <div class=" col-lg-6 col-md-6 col-sm-6 col-xs-6">
            <div class="">
                <div class="">
                    <label>
                        Valor Categoria Medicamento
                    </label>
                    <input class="form-control"
                           id="txtValorCatMedicamento" required />
                </div>
            </div>
        </div>
    </div>

</div>
<br />
<div class="container">

    <button type="button" class="btn btn-success" onclick="guardar();">Guardar</button>
    <button type="button" class="btn btn-success" onclick="buscar();">Buscar</button>
    <button type="button" class="btn btn-success" onclick="guardar();">Modificar</button>
    <button type="button" class="btn btn-success" onclick="eliminar();">Eliminar</button>
    <button type="button" class="btn btn-success" onclick="limpiar();">Limpiar</button>

</div>

<br />
<div class="container">
    <h2>Lilstado</h2>
</div>
<div class="table-responsive" id="listado"></div>

<div class="container">
    <!-- <h2>Modal Example</h2>
      Trigger the modal with a button
     <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#myModal">Open Modal</button>-->
    <!-- Modal -->
    <div class="modal fade" id="myModal" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                @*<div class="modal-header">*@
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h1 class="modal-title">Aviso</h1>
                @*</div>*@
                <div class="modal-body">
                    <span id="avisos"></span>
                </div>
                <div class="row">
                    @*<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>*@
                </div>
            </div>

        </div>
    </div>

</div>
